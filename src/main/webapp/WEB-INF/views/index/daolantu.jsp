<script>
    $(function(){
    	$('#tu').scrollTop(800);
    	$('#tu').scrollLeft(1800);
    $('body').click(function(e){
    	$('.daolantu_tc').hide();
    })
    $('.tc_wrap').click(function(e){
	    e.stopPropagation();
	    var winW=$(window).width();
	    if(winW<640){
		    if($(this).hasClass('play')){
			    $(this).removeClass('play');
			    $(this).find('audio').remove();
			   
		    }else{
			    $('.tc_wrap').removeClass('play');
			    $('audio').remove();
			    $(this).addClass('play');
			    var src=$(this).attr('data-audio');
			    var audio='<audio src="${ctx}/static/audio/'+src+'.mp3">亲 您的浏览器不支持html5的audio标签</audio>';
			    $(this).append(audio);
			    $(this).find('audio')[0].controls = 'controls';
			    $(this).find('audio')[0].play();
		    }
    	}else{
	    $('.daolantu_tc').hide();
	    $(this).find('.daolantu_tc').show();
    }

    })
    })
</script>
